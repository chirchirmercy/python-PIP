#.append 
add new item to bottom of the list

names=["mercy","rose","caren"]
names.append("faith")
print(names)

output=["mercy","caren","faith"]


#.extend
add new list to current list

names=["grace","rose","caren"]
names2=("cathy","ann")
names.extend(names2)
print(names)

output=["grace","rose","caren","cathy","ann"]

#.insert
insert a value/string to a given index position

schools=["pangani","mugumo","koroto","torongo"]
schools.insert(3,"kabarak")
print(schools)

output=["pangani","mugumo","kabarak","koroto","torongo"]

#pop
removes and return last item in a list
places=["nakuru","nairobi","kitale"]
places .pop()
print(places)

output="kitale"

//slicing-accessing range of characters in a string through their index position

s= "python"
s[1:4]
output="yth"


d="Akirachix"
[5:]
output="chix"


